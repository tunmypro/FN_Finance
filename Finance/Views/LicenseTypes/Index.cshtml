@model IEnumerable<Finance.Models.LicenseTypes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InterestName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InterestRate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InterestName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InterestRate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LicenseType }) |
            @Html.ActionLink("Details", "Details", new { id=item.LicenseType }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LicenseType })
        </td>
    </tr>
}

</table>
